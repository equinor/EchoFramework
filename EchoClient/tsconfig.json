{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "allowJs": true,
    "skipLibCheck": true,
    "target": "es5",
    "resolveJsonModule": true,
    "types": ["@equinor/echo-scripts", "@types/node"],
  },
  "include": [
    "src"
  ]

}

